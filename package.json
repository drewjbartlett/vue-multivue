      "es2015"
    ]
  },
  "bugs": {
    "url": "https://github.com/drewjbartlett/vue-multivue/issues"
  },
  "homepage": "https://github.com/drewjbartlett/vue-multivue#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1"
  }
